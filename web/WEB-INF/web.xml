<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>shipmentSearchServlet</servlet-name>
        <servlet-class>ASD.SmartCard.Controller.shipmentSearchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShipmentEditServlet</servlet-name>
        <servlet-class>ASD.SmartCard.Controller.ShipmentEditServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>shipmentUpdateServlet</servlet-name>
        <servlet-class>ASD.SmartCard.Controller.shipmentUpdateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ShipmentDeleteServlet</servlet-name>
        <servlet-class>ASD.SmartCard.Controller.ShipmentDeleteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AllShipmentsServlet</servlet-name>
        <servlet-class>ASD.SmartCard.Controller.AllShipmentsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewShipmentServlet</servlet-name>
        <servlet-class>ASD.SmartCard.Controller.NewShipmentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewAdminServlet</servlet-name>
        <servlet-class>ASD.SmartCard.Controller.NewAdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AllAdminServlet</servlet-name>
        <servlet-class>ASD.SmartCard.Controller.AllAdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendEmail</servlet-name>
        <servlet-class>ASD.Email.Contoller.SendEmail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserVerification</servlet-name>
        <servlet-class>ASD.Email.Contoller.UserVerification</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerifyCode</servlet-name>
        <servlet-class>ASD.Email.Contoller.VerifyCode</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>shipmentSearchServlet</servlet-name>
        <url-pattern>/shipmentSearchServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShipmentEditServlet</servlet-name>
        <url-pattern>/ShipmentEditServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>shipmentUpdateServlet</servlet-name>
        <url-pattern>/shipmentUpdateServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ShipmentDeleteServlet</servlet-name>
        <url-pattern>/ShipmentDeleteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AllShipmentsServlet</servlet-name>
        <url-pattern>/AllShipmentsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewShipmentServlet</servlet-name>
        <url-pattern>/NewShipmentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewAdminServlet</servlet-name>
        <url-pattern>/NewAdminServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AllAdminServlet</servlet-name>
        <url-pattern>/AllAdminServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendEmail</servlet-name>
        <url-pattern>/SendEmail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserVerification</servlet-name>
        <url-pattern>/UserVerification</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerifyCode</servlet-name>
        <url-pattern>/VerifyCode</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
